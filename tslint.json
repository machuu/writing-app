{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "max-classes-per-file": [
        false
      ],
      "member-ordering": [
        true,
        {
          "order": "instance-sandwich"
        }
      ],
      "no-console": true,
      "variable-name": {
        "options": [
          "allow-leading-underscore"
        ]
      }
    },
    "rulesDirectory": []
}
